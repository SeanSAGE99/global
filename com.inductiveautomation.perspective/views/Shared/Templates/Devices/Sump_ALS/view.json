{
  "custom": {
    "controlEnable": true,
    "deviceTagPath": "Alexandrina/Mt Compass/WWPS/MCP05/Sump",
    "max": null,
    "min": 0,
    "refSourcePath": null
  },
  "params": {
    "controlEnable": true,
    "deviceName": "Sump",
    "deviceParentPath": "Alexandrina/Mt Compass/WWPS/MCP05",
    "shownOnOverview": true
  },
  "propConfig": {
    "custom.controlEnable": {
      "binding": {
        "config": {
          "path": "view.params.controlEnable"
        },
        "type": "property"
      },
      "persistent": true
    },
    "custom.deviceTagPath": {
      "binding": {
        "config": {
          "expression": "{view.params.deviceParentPath} + \u0027/\u0027 +\r\n{view.params.deviceName}"
        },
        "type": "expr"
      },
      "persistent": true
    },
    "custom.max": {
      "binding": {
        "config": {
          "fallbackDelay": 2.5,
          "mode": "indirect",
          "references": {
            "deviceTagPath": "{view.custom.deviceTagPath}"
          },
          "tagPath": "{deviceTagPath}/ALS/Setpoints/High High Alarm"
        },
        "transforms": [
          {
            "expression": "{value} + 100",
            "type": "expression"
          }
        ],
        "type": "tag"
      },
      "persistent": true
    },
    "custom.min": {
      "persistent": true
    },
    "custom.refSourcePath": {
      "binding": {
        "config": {
          "fallbackDelay": 2.5,
          "mode": "indirect",
          "references": {
            "1": "{view.custom.deviceTagPath}"
          },
          "tagPath": "{1}/ALS/PV.SourceTagPath"
        },
        "type": "tag"
      },
      "persistent": true
    },
    "params.controlEnable": {
      "paramDirection": "input",
      "persistent": true
    },
    "params.deviceName": {
      "paramDirection": "input",
      "persistent": true
    },
    "params.deviceParentPath": {
      "paramDirection": "input",
      "persistent": true
    },
    "params.shownOnOverview": {
      "paramDirection": "input",
      "persistent": true
    }
  },
  "props": {
    "defaultSize": {
      "height": 47,
      "width": 67
    },
    "dropConfig": {
      "udts": [
        {
          "action": "path",
          "param": "",
          "type": "Devices/Motors/DOL-DOL_SS"
        }
      ]
    }
  },
  "root": {
    "children": [
      {
        "children": [
          {
            "events": {
              "dom": {
                "onClick": {
                  "config": {
                    "script": "\tshared.perspective.device.openDevicePopup(self,event)"
                  },
                  "scope": "G",
                  "type": "script"
                }
              }
            },
            "meta": {
              "name": "Button"
            },
            "position": {
              "height": 1,
              "width": 1
            },
            "propConfig": {
              "meta.visible": {
                "binding": {
                  "config": {
                    "expression": "{view.params.enablePopup}"
                  },
                  "type": "expr"
                }
              }
            },
            "props": {
              "style": {
                "backgroundColor": "transparent",
                "borderStyle": "none",
                "cursor": "pointer"
              },
              "text": ""
            },
            "type": "ia.input.button"
          },
          {
            "meta": {
              "name": "Sump_01_opt"
            },
            "position": {
              "height": 1,
              "width": 1
            },
            "props": {
              "style": {
                "borderBottomLeftRadius": "3px",
                "borderBottomRightRadius": "3px",
                "borderBottomStyle": "solid",
                "borderBottomWidth": "3px",
                "borderLeftStyle": "solid",
                "borderLeftWidth": "3px",
                "borderRightStyle": "solid",
                "borderRightWidth": "3px",
                "borderTopStyle": "none",
                "color": "var(--neutral-70)"
              }
            },
            "type": "ia.display.label"
          },
          {
            "custom": {
              "tags": [
                {
                  "aggregate": "Average",
                  "alias": "Level"
                }
              ]
            },
            "meta": {
              "name": "Sparkline"
            },
            "position": {
              "height": 0.8936,
              "width": 0.9104,
              "x": 0.0497,
              "y": 0.0496
            },
            "propConfig": {
              "custom.tags[0].path": {
                "binding": {
                  "config": {
                    "path": "view.custom.refSourcePath"
                  },
                  "type": "property"
                }
              },
              "props.points": {
                "binding": {
                  "config": {
                    "aggregate": "MinMax",
                    "avoidScanClassValidation": true,
                    "dateRange": {
                      "mostRecent": "{session.custom.history.sparklineCharts.lastMins}",
                      "mostRecentUnits": "MIN"
                    },
                    "ignoreBadQuality": false,
                    "polling": {
                      "enabled": true,
                      "rate": "{session.custom.history.sparklineCharts.updatePeriodSecs}"
                    },
                    "preventInterpolation": false,
                    "returnFormat": "Wide",
                    "returnSize": {
                      "numRows": "100",
                      "type": "FIXED"
                    },
                    "tags": "{this.custom.tags}",
                    "valueFormat": "DATASET"
                  },
                  "type": "tag-history"
                }
              },
              "props.range.high": {
                "binding": {
                  "config": {
                    "path": "view.custom.max"
                  },
                  "type": "property"
                }
              },
              "props.range.low": {
                "binding": {
                  "config": {
                    "path": "view.custom.min"
                  },
                  "type": "property"
                }
              }
            },
            "props": {
              "color": "#1F65DF",
              "desired": {
                "fill": {
                  "opacity": 0
                },
                "high": 5,
                "low": 4,
                "stroke": {
                  "dashArray": 0,
                  "opacity": 0
                }
              },
              "marker": {
                "first": {
                  "size": 0
                },
                "high": {
                  "size": 0
                },
                "last": {
                  "size": 0
                },
                "low": {
                  "size": 0
                }
              },
              "range": {}
            },
            "type": "ia.display.sparkline"
          }
        ],
        "meta": {
          "name": "CoordinateContainer"
        },
        "position": {
          "grow": 1
        },
        "props": {
          "mode": "percent",
          "style": {
            "overflow": "visible"
          }
        },
        "type": "ia.container.coord"
      }
    ],
    "meta": {
      "name": "root"
    },
    "props": {
      "direction": "column",
      "style": {
        "overflow": "hidden"
      }
    },
    "type": "ia.container.flex"
  }
}