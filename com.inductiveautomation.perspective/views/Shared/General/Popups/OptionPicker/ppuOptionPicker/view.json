        "scripts": {
          "customMethods": [
            {
              "name": "refreshOptions",
              "params": [],
              "script": "\tself.props.instances \u003d []\n\tavailType \u003d self.view.params.availType\n\toptions_raw \u003d self.view.custom.table_raw\n\tif availType \u003d\u003d \u0027bit\u0027:\n\t\toptions_raw \u003d system.dataset.toPyDataSet(options_raw)\n\t\n\toptions \u003d []\n\tfor i, option_raw in enumerate(options_raw):\n\t\toption \u003d {\u0027optionValue\u0027: option_raw[\u0027Value\u0027],\n\t\t\t  \t  \u0027optionLabel\u0027: option_raw[\u0027Label\u0027],\n\t\t\t\t  \u0027selectTagPath\u0027: self.view.params.selectTagPath,\n\t\t\t\t  \u0027availType\u0027: availType}\n\t\t\n\t\tif availType \u003d\u003d \u0027table\u0027:\n\t\t\tavail \u003d option_raw[\u0027Avail\u0027]\n\t\t\toption[\u0027tableAvail\u0027] \u003d {\u0027avail\u0027: avail} \n\t\t\toption[\u0027bitAvail\u0027] \u003d {\u0027bit\u0027: \u0027\u0027, \u0027integerAvailTagPath\u0027: \u0027\u0027}\n\t\t\t\n\t\t\toption[\u0027tableAvail_avail\u0027] \u003d avail\n\t\t\toption[\u0027bitAvail_bit\u0027] \u003d \u0027\u0027\n\t\t\toption[\u0027bitAvail_integerAvailTagPath\u0027] \u003d \u0027\u0027\n\t\t\t\n\t\telif availType \u003d\u003d \u0027bit\u0027:\n\t\t\tavailBit \u003d option_raw[\u0027AvailableBit\u0027]\n\t\t\toption[\u0027tableAvail\u0027] \u003d {\u0027avail\u0027: \u0027\u0027} \n\t\t\toption[\u0027bitAvail\u0027] \u003d {\u0027bit\u0027: availBit, \u0027integerAvailTagPath\u0027: self.view.params.bitAvail.integerAvailTagPath}\n\t\t\t\n\t\t\toption[\u0027tableAvail_avail\u0027] \u003d \u0027\u0027\n\t\t\toption[\u0027bitAvail_bit\u0027] \u003d availBit\n\t\t\toption[\u0027bitAvail_integerAvailTagPath\u0027] \u003d self.view.params.bitAvail.integerAvailTagPath\n\t\t\n\t\telse:\n\t\t\toption[\u0027tableAvail\u0027] \u003d {\u0027avail\u0027: \u0027\u0027} \n\t\t\toption[\u0027bitAvail\u0027] \u003d {\u0027bit\u0027: \u0027\u0027, \u0027integerAvailTagPath\u0027: \u0027\u0027}\n\t\t\t\n\t\t\toption[\u0027tableAvail_avail\u0027] \u003d \u0027\u0027\n\t\t\toption[\u0027bitA