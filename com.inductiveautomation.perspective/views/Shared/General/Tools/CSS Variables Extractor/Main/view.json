      "customMethods": [
        {
          "name": "refresh",
          "params": [],
          "script": "\tdef parseCSS(themeParentPath, absFilePath, cssFiles\u003dNone):\n\t\tif cssFiles\u003d\u003dNone: cssFiles\u003d[]\n\t\timport re\n\t\tprint \u0027absFilePath: \u0027, absFilePath\n\t\tabsFilePath \u003d str(absFilePath)\n\t\tfileName \u003d absFilePath.split(\u0027/\u0027)[-1]\n\t\tcssText \u003d system.file.readFileAsString(absFilePath).split(\u0027\\n\u0027)\n\t\tinsideRoot \u003d False\n\t\tcssFile \u003d {}\n\t\tvars \u003d []\n\t\trelPath \u003d \u0027./\u0027 + absFilePath.replace(themeParentPath, \u0027\u0027)\n\t\tfor line in cssText:\n\t\t\tif fileName \u003d\u003d \u0027variables.css\u0027:\n\t\t\t\tif insideRoot:\n\t\t\t\t\tline \u003d line.strip()\n\t\t\t\t\tif line[:2] \u003d\u003d \u0027--\u0027:\n\t\t\t\t\t\tvarName \u003d line.split(\":\")[0]\n\t\t\t\t\t\tvarValue \u003d re.split(\":|;\", line)[1]\n\t\t\t\t\t\tvars.append({\u0027name\u0027:varName, \u0027value\u0027:varValue})\n\t\t\t\t\t\t\t\t\t\t\n\t\t\t\tif line.find(\u0027:root\u0027) !\u003d -1:\n\t\t\t\t\tinsideRoot \u003d True\n\t\t\t\tif insideRoot and line.find(\"}\") !\u003d -1:\n\t\t\t\t\tinsideRoot \u003d False\n\t\t\t\n\t\t\tif line[:7] \u003d\u003d \u0027@import\u0027:\n\t\t\t\trelPath \u003d line.replace(\u0027@import \"\u0027,\u0027\u0027).replace(\u0027\";\u0027, \u0027\u0027)\n\t\t\t\t# E.g. ./variable.css\n\t\t\t\t#      ../light/index.css\n\t\t\t\tdots \u003d relPath.split(\u0027/\u0027)[0]\n\t\t\t\tdotCount \u003d dots.count(\".\")\n\t\t\t\tabsPath \u003d \"/\".join(absFilePath.split(\"/\")[:-1*dotCount]) + \u0027/\u0027 + \u0027/\u0027.join(relPath.split(\u0027/\u0027)[1:])\n\t\t\t\tcssFiles \u003d parseCSS(themeParentPath, absPath, cssFiles)\n\t\t\n\t\trelPath \u003d \u0027./\u0027 + absFilePath.replace(themeParentPath, \u0027\u0027)\n\t\tfor file in cssFiles:\n\t\t\tif file[\u0027fileRelPath\u0027] \u003d\u003d relPath:\n\t\t\t\treturn cssFiles\n\t\tif fileName \u003d\u003d \u0027variables.css\u0027:\n\t\t\tcssFiles.append({\n\t\t\t\t\t\t\t\u0027fileRelPath\u0027:relPath,\n\t\t\t\t\t\t\t\u0027variables\u0027:vars\n\t\t\t\t\t\t\t})\n\t\treturn cssFiles\n\t\n\tdef parseThemeCSS(themeName\u003d\u0027\u0027):\n\t\tif themeName \u003d\u003d \u0027\u0027:\n\t\t\tthemeName \u003d self.session.props.theme\n\t\tfrom com.inductiveautomation.ignition.gateway import IgnitionGateway\n\t\tcontext \u003d IgnitionGateway.get()\n\t\tprint \u0027CSS_EXTRACTOR:\u0027, context.systemManager.dataDir.absoluteFile\n\t\tthemeParentPath \u003d str(context.systemManager.dataDir.absoluteFile).replace(\u0027\\\\\u0027,\u0027/\u0027) + \u0027/modules/com.inductiveautomation.perspective/themes/\u0027\n\t\tprint \u0027CSS_EXTRACTOR: themeParentPath \u003d \u0027, themeParentPath\n\t\t#themeParentPath \u003d \u0027c:/Program Files/Inductive Automation/Ignition/data/modules/com.inductiveautomation.perspective/themes/\u0027\n\t\tabsFilePath \u003d \u0027%s%s.css\u0027 % (themeParentPath, themeName)\n\t\tprint \u0027CSS_EXTRACTOR: absFilePath \u003d \u0027, absFilePath\n\t\tjson \u003d parseCSS(themeParentPath, absFilePath)\n\t\treturn json\n\t\n\tself.getChild(\"FlexRepeater\").props.instances \u003d parseThemeCSS()"
        }
      ],
      "extensionFunctions": null,
      "messageHandlers": []
    },
