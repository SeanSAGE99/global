{
  "base": {
    "animation": {
      "duration": "0.25s",
      "timingFunction": "cubic-bezier(0.68, -0.55, 0.265, 1.55)",
      "keyframes": {
        "0%": {
          "backgroundColor": "var(--dev-txtbg-stopped)",
          "color": "var(--dev-txt-stopped)",
          "fill": "var(--dev-sym-stopped)"
        },
        "100%": {
          "backgroundColor": "var(--dev-txtbg-stopped-flash)",
          "color": "var(--dev-txt-stopped-flash)",
          "fill": "var(--dev-sym-stopped-flash)"
        }
      }
    }
  }
}